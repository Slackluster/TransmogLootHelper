<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\Blizzard_SharedXML\UI.xsd">
	<Frame name="TransmogLootHelper_SettingsText" mixin="TransmogLootHelper_SettingsTextMixin" virtual="true">
		<Size y="28"/>
		<Layers>
			<Layer level="OVERLAY">
				<FontString parentKey="Text" inherits="GameFontNormal" justifyH="LEFT" justifyV="TOP">
					<Anchors>
						<Anchor point="TOPLEFT" x="37" y="-8"/>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
	</Frame>

	<Button name="TransmogLootHelper_ListButton" virtual="true">
		<Size x="200" y="16"/>
		<HighlightTexture file="Interface\QuestFrame\UI-QuestTitleHighlight" alphaMode="ADD" setAllPoints="true"/>
		<Layers>
			<Layer level="OVERLAY">
				<FontString parentKey="LeftText1" inherits="GameFontNormal" justifyH="LEFT">
					<Anchors>
						<Anchor point="LEFT" x="0"/>
					</Anchors>
					<Size x="400" y="24"/>
				</FontString>
				<FontString parentKey="LeftText2" inherits="GameFontNormal" justifyH="LEFT" wordwrap="false">
					<Anchors>
						<Anchor point="LEFT" x="18"/>
						<Anchor point="RIGHT" x="-18"/>
					</Anchors>
					<Size x="400" y="24"/>
				</FontString>
				<FontString parentKey="RightText" inherits="GameFontNormal" justifyH="RIGHT">
					<Anchors>
						<Anchor point="RIGHT" x="-5"/>
					</Anchors>
					<Size x="400" y="24"/>
				</FontString>
			</Layer>
		</Layers>
	</Button>
</Ui>
